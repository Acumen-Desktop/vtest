{
	"name": "my-new-app",
	"productName": "my-new-app",
	"version": "1.0.0",
	"description": "My Electron application description",
	"main": ".vite/build/main/main.js",
	"type": "module",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "node --no-deprecation ./node_modules/@electron-forge/cli/dist/electron-forge.js make",
		"publish": "electron-forge publish",
		"svelte-dev": "vite -c vite/vite.renderer.config.ts",
		"svelte-build": "vite build -c vite/vite.renderer.config.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"update:deps": "npx npm-check-updates -u && npm install",
		"copy-icon": "tsx src/lib/utils/copy-icons.ts"
	},
	"devDependencies": {
		"@codemirror/lang-javascript": "6.2.2",
		"@codemirror/theme-one-dark": "6.1.2",
		"@electron-forge/cli": "^7.6.0",
		"@electron-forge/maker-deb": "^7.6.0",
		"@electron-forge/maker-rpm": "^7.6.0",
		"@electron-forge/maker-squirrel": "^7.6.0",
		"@electron-forge/maker-zip": "^7.6.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
		"@electron-forge/plugin-fuses": "^7.6.0",
		"@electron/fuses": "^1.8.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.8.5",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"@types/electron-squirrel-startup": "^1.0.2",
		"@vscode/codicons": "^0.0.36",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.64",
		"clsx": "^2.1.1",
		"electron": "33.2.0",
		"globals": "^15.12.0",
		"lucide-svelte": "^0.462.0",
		"paneforge": "^1.0.0-next.1",
		"postcss": "^8.4.49",
		"svelte": "^5.2.9",
		"svelte-check": "^4.1.0",
		"svelte-codemirror-editor": "1.4.1",
		"tailwind-merge": "^2.5.5",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.15",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "10.9.2",
		"typescript": "^5.7.2",
		"vite": "^6.0.1",
		"vitest": "^2.1.6",
		"xterm": "^5.3.0",
		"xterm-addon-fit": "^0.8.0"
	},
	"keywords": [],
